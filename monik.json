{
  "Common":
  {
    "ServerUrl": "http://url/",
    "TimeTemplate": "HH:mm",
    "RefreshPeriod": 5,
    "Mode" : "stream"
  },
  "Log":
  {
    "MaxSourceLen": 12,
    "MaxInstanceLen": 8,
    "LevelFilter": "none",
    "SeverityCutoff": "info",
    "ShowLevelVerbose": true,
    "Top": 20,
  },
  "KeepAlive":
  {
    "MaxSourceLen": 12,
    "MaxInstanceLen": 8,
    "KeepAliveWarnSeconds": 60,
    "Request": {
      "Groups":[],
      "Instances":[]
    }
  },
  "MetricsFill":
  {
    "Areas": "[
      {\"Range\": [null, 0], \"Color\": \"DarkGreen\", \"FontColor\": \"Black\"},
      {\"Range\": [0, 1], \"Color\": \"DarkYellow\"},
      {\"Range\": [1, null], \"Color\": \"DarkRed\"}
    ]",
    "ValueFormatGauge": "0.#",
    "ValueFormatAccum": null
  },
  "Metrics":
  {
    "MaxMetricLen": 12,
    "MaxMetricValueLen": 8,
    "MaxAggregationTypeLen": 3,
    "DefaultValueFormat": "0.#",
    "Metrics": [
      {
        "MetricId": 1,
        "ValueFormat": "##0.0",
        "Areas": [
          {"Range": [30, null], "Color": "DarkRed"},
          {"Range": [20, 30], "Color": "DarkYellow"},
          {"Range": [10, 20], "Color": "DarkGreen", "FontColor": "Black"},
          {"Range": [null, 10], "Color": "Black"}
        ]
      },
      {
        "MetricId": 2,
        "Areas": [
          {"Range": [null, 0], "Color": "DarkGreen", "FontColor": "Black"},
          {"Range": [0, 0.5], "Color": "DarkYellow"},
          {"Range": [0.5, null], "Color": "DarkRed"}
        ]
      }
    ],
    "MetricsAutoFilled": [
    ]
  }
}
